pipeline {
    agent any

    environment {
        PATH = "/opt/apache-maven-3.9.11/bin:$PATH"
        // Make sure SONAR_TOKEN is securely stored as a Jenkins secret
    }

    stages {
        stage('Build & Analyze') {
            steps {
                withSonarQubeEnv('sonarqube-server') {
                    sh """
                    mvn clean verify sonar:sonar \\
                        -Dsonar.projectKey=mydemoorg3_mydemoproject3 \\
                        -Dsonar.host.url=https://sonarcloud.io/ \\
                        -Dsonar.login=${sonarqube-cred}
                    """
                }
            }
        }
    }
}



       
               
           
       
    

